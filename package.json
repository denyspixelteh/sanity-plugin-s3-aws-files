{
  "name": "sanity-plugin-s3-aws-files",
  "version": "1.0.2",
  "description": "Store Sanity media files in AWS S3",
  "scripts": {
    "clear-lib": "node clearLib.js",
    "build": "npm run clear-lib && tsc",
    "dev": "concurrently \"tsc -w\" \"tsc --module esnext --outDir build/esm -w\""
  },
  "main": "build/index.jsx",
  "types": "build/index.d.ts",
  "exports": {
    ".": {
      "types": "./build/index.d.ts",
      "import": "./build/index.js",
      "default": "./build/index.js"
    },
    "./package.json": "./package.json"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/denyspixelteh/sanity-plugin-s3-aws-files.git"
  },
  "dependencies": {
    "@sanity/client": "^6.21.3",
    "@sanity/color": "^3.0.6",
    "@sanity/icons": "^3.4.0",
    "@sanity/image-url": "^1.0.2",
    "@sanity/schema": "^3.57.4",
    "@sanity/types": "^3.57.4",
    "@sanity/ui": "2.8.9",
    "@xstate/react": "^3.2.2",
    "clipboard": "^2.0.11",
    "lodash-es": "^4.17.21",
    "mime": "^4.0.4",
    "nanoid": "^5.0.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-dropzone": "^14.2.3",
    "react-rx": "^4.0.0",
    "sanity": "^3.57.4",
    "styled-components": "^6.1.13",
    "xstate": "^4.38.3"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.12",
    "@types/mime": "^4.0.0",
    "@types/react": "^18.3.7",
    "@types/react-dom": "^18.3.0",
    "@types/styled-components": "5.1.34",
    "concurrently": "^9.0.1",
    "typescript": "^5.6.2"
  }
}
